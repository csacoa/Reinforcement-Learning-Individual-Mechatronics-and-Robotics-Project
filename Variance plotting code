######
#This code is for showing the variance in the results of an experiment
#####


import pandas as pd
import matplotlib.pyplot as plt


data = pd.read_csv('output.csv', header=None, names=['values'])

plt.scatter(data.index, data['values'], s=5)

plt.xlabel('Trial Number')
plt.ylabel('Score')
plt.title('Inverted double pendulum scores PPO 10 million episodes of training')

plt.show()
